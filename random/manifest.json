{
    "name": "Random",
    "version": "1.0",
    "description": "Flip a coin, roll a dice, etc.",
    "manifest_version": 2,
    "author": "Nat Dring",

    "permissions": [
        "storage"
      ],

    "background": {
        "scripts": ["background.js"],
        "persistent": false
      },

    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "128": "images/img.png"
        }
    },

    "icons": {
        "128": "images/img.png"
    }
}